{
  "name": "another-cache",
  "version": "0.1.1",
  "description": "A lightweight, flexible cache library for JavaScript that works seamlessly in both Node.js and browser environments",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:bundle",
    "build:clean": "rm -rf dist",
    "build:bundle": "rollup -c",
    "prepublishOnly": "npm run build",
    "dev": "rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "another-cache",
    "cache",
    "caching",
    "memory",
    "storage",
    "node",
    "browser",
    "typescript",
    "lru",
    "fifo",
    "ttl",
    "events",
    "in-memory"
  ],
  "author": "Oscar Throedsson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oscarthroedsson/anothercache"
  },
  "bugs": {
    "url": "https://github.com/oscarthroedsson/anothercache/issues"
  },
  "homepage": "https://github.com/oscarthroedsson/anothercache",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rollup": "^4.55.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.9.3"
  }
}
